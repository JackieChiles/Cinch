{{#paper-card}}
    {{#paper-card-content}}
        <div class="username">
            {{user.name}}
        </div>

        <div class="current-bid">
            Bid: {{currentBid}}
        </div>

        {{#each cards as |card|}}
            {{card-image card=card}}
        {{/each}}
    {{/paper-card-content}}
{{/paper-card}}
